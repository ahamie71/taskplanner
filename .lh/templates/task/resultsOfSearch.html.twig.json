            "content": "{% extends 'base.html.twig' %}\n\n{% block title %}Hello SearchController!{% endblock %}\n\n{% block body %}\n\n{{form(form)}}\n\n{% if articles | length == 0 %}\n            <h4>Aucun résulat pour votre recherche.</h4>\n\n            {% else %}\n               <h3 class=\"mt-3\">Vos résulats de recherche :</h3>\n                 \n                 {% for article in articles %}\n                  <article>\n\n                 \n  \n      <div class=\"card \" style=\"background-color:yellow\">\n              <img src=\"{{article.image}}\" alt=\"\"style=\"width: 150px;\">\n      <h2>{{article.title}}  </h2>\n      <div class=\"metadata\">Ecrit le {{article.createdAT | date('d/m/Y') }} à {{article.createdAt | date('H:i')}}  dans la  catégorie {{article.category.title}}</div>\n       <div class=\"centent\">\n\n         {{article.content | raw}}\n           \n     <a href=\"{{path('prog_show',{'id':article.id}) }}\" class=\"btn btn-primary\">les commentaires</a>\n     </div>\n                      {% endfor %}\n               {% endif %}\n\n\n{% endblock %}\n"
